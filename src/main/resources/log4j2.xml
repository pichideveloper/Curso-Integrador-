<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
    <Appenders>
        <File name="LogFile" fileName="logs/app.log">
            <PatternLayout pattern="%d [%t] %-5level - %msg%n"/>
        </File>
    </Appenders>

    <Loggers>
        <Root level="info">
            <AppenderRef ref="LogFile"/>
        </Root>
    </Loggers>
</Configuration>
